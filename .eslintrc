{
  "extends": [
    "next",
    "next/core-web-vitals"
  ],
  "rules": {
    "no-console": "warn",
    "semi": [
      "error",
      "never"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "jsx-quotes": [
      "error",
      "prefer-double"
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ]
  }
}
